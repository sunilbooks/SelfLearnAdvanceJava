<html>
<body>
	<form action="upload" method="POST"
		enctype="multipart/form-data">

		<table border="0" cellspacing="0">
			<tr>
				<td colspan="2"><b>Upload Files</b></td>
			</tr>
			<tr>
				<td>Title</td>
				<td><input name="title" size="20"></td>
			</tr>
			<tr>
				<td>Subtitle</td>
				<td><input name="subtitle" size="20"></td>
			</tr>
			<tr>
				<td>Image File</td>
				<td><input name="doc1" type="file"></td>
			</tr>
			<tr>
			<tr>
				<td>Text File</td>
				<td><input name="doc2" type="file"></td>
			</tr>
			<tr>
				<td colspan="2" align="left"><input type="submit" value="Send"></td>
			</tr>

		</table>
	</form>

</body>
</html>
